{
  "name": "maskom-network",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "pages:dev": "wrangler pages dev ./dist",
    "deploy:cloudflare": "node scripts/deploy-enhanced.js",
    "deploy:cloudflare:simple": "npm run build && wrangler pages deploy dist --compatibility-date=2024-09-23",
    "security:audit": "npm audit --audit-level moderate",
    "security:audit:critical": "npm audit --audit-level critical",
    "security:audit:full": "npm audit --audit-level low",
    "security:check": "npm audit && npm outdated",
    "security:fix": "npm audit fix",
    "security:scan": "npm run security:audit && npm run lint && npm run typecheck",
    "simulate-bandwidth": "tsx src/scripts/bandwidth-simulation.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "ci": "npm run lint && npm run typecheck && npm run test:run && npm run build",
    "deploy:verify": "node scripts/verify-deployment.js"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.10",
    "@astrojs/node": "^9.5.0",
    "@astrojs/sitemap": "^3.6.0",
    "@fontsource/cal-sans": "^5.2.3",
    "@fontsource/inter": "^5.2.8",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "astro": "^5.15.8",
    "astro-icon": "^1.1.5",
    "astro-seo": "^0.8.4",
    "chart.js": "^4.5.1",
    "openai": "^6.9.0",
    "supabase": "^2.58.5",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.5",
    "@eslint/js": "^9.0.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "axios": "^1.13.2",
    "eslint": "^9.0.0",
    "eslint-plugin-astro": "^1.5.0",
    "form-data": "^4.0.4",
    "globals": "^16.5.0",
    "js-yaml": "^4.1.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.2.0",
    "prettier-plugin-astro": "^0.14.0",
    "tsx": "^4.6.0",
    "typescript-eslint": "^8.0.0",
    "undici": "^7.16.0",
    "vitest": "^4.0.9",
    "wrangler": "^4.46.0"
  },
  "overrides": {
    "undici": "^7.16.0",
    "axios": "^1.13.2",
    "form-data": "^4.0.4",
    "js-yaml": "^4.1.1"
  }
}
