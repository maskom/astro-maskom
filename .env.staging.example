# Staging Environment Configuration
# Copy this file to .env.staging and update with staging-specific values

# Supabase Configuration (Staging)
SUPABASE_URL=your_staging_supabase_url
SUPABASE_ANON_KEY=your_staging_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_staging_supabase_service_role_key

# Cloudflare Configuration
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token

# Application Configuration
NODE_ENV=staging
SITE_URL=https://astro-maskom-staging.pages.dev

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CHATBOT=false
ENABLE_BILLING=false

# Security
SECURITY_WEBHOOK_SECRET=your_staging_webhook_secret